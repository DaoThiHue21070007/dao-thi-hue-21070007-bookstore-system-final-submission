<div id="<%= dom_id staff %>">
  <p>
    <strong>Code:</strong>
    <%= staff.code %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= staff.name %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= staff.gender_id %>
